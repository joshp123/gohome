      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "type": "range",
              "options": {
                "from": -1000000000.0,
                "to": -0.0001,
                "result": {
                  "text": "Producing"
                }
              }
            },
            {
              "type": "range",
              "options": {
                "from": -0.0001,
                "to": 0.0001,
                "result": {
                  "text": "Idle"
                }
              }
            },
            {
              "type": "range",
              "options": {
                "from": 0.0001,
                "to": 1000000000.0,
                "result": {
                  "text": "Consuming"
                }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "orange",
                "value": 0.0001
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 8,
