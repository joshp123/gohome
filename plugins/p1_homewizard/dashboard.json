      "datasource": "VictoriaMetrics",
      "fieldConfig": {
        "defaults": {
          "unit": "kwh"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 16,
        "y": 0
      },
      "id": 4,
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "increase(gohome_p1_homewizard_total_export_kwh[24h])",
          "refId": "A"
        }
      ],
      "title": "Produced (24h)",
      "type": "stat"
    },
    {
      "datasource": "VictoriaMetrics",
      "fieldConfig": {
        "defaults": {
          "unit": "currencyEUR"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 20,
        "y": 0
      },
      "id": 5,
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "(increase(gohome_p1_homewizard_total_import_t1_kwh[24h]) * gohome_p1_homewizard_tariff_import_t1_eur_per_kwh) + (increase(gohome_p1_homewizard_total_import_t2_kwh[24h]) * gohome_p1_homewizard_tariff_import_t2_eur_per_kwh) + - (increase(gohome_p1_homewizard_total_export_t1_kwh[24h]) * gohome_p1_homewizard_tariff_export_t1_eur_per_kwh) + - (increase(gohome_p1_homewizard_total_export_t2_kwh[24h]) * gohome_p1_homewizard_tariff_export_t2_eur_per_kwh)",
          "refId": "A"
        }
      ],
      "title": "Cost (24h)",
      "type": "stat"
    },
    {
      "datasource": "VictoriaMetrics",
      "fieldConfig": {
        "defaults": {
          "unit": "kwh",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 50,
            "lineWidth": 0
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Consumed"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "#73BF69"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Produced"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "#E0B400"
                }
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "id": 6,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        }
      },
      "targets": [
        {
          "expr": "increase(gohome_p1_homewizard_total_import_kwh[$__rate_interval])",
          "legendFormat": "Consumed",
          "refId": "A"
        },
        {
          "expr": "-1 * increase(gohome_p1_homewizard_total_export_kwh[$__rate_interval])",
          "legendFormat": "Produced",
          "refId": "B"
        }
      ],
      "title": "Energy flow (24h)",
      "type": "timeseries"
    }
  ],
  "refresh": "10s",
