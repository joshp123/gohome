      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 12,
        "y": 0
      },
      "id": 3,
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "increase(gohome_p1_homewizard_total_import_kwh[24h])",
          "refId": "A"
        }
